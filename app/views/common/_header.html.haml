.left
  = link_to(image_tag("qfdonline_logo.jpg"), root_path)

.right
  - if logged_in?
    Welcome #{h current_user.email}
    %br
    = link_to("My Profile", account_path)
    |
    = link_to("Log out", logout_path)
  - else
    = link_to("Register", register_path)
    |
    = link_to("Log in", login_path)
